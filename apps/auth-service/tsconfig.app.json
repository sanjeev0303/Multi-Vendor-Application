{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "out-tsc/auth-service",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": [
      "node", "express"
    ],
    "rootDir": "../../",
    "tsBuildInfoFile": "out-tsc/auth-service/tsconfig.app.tsbuildinfo"
  },
  "include": [
    "src/**/*.ts",
    "../../packages/**/*.ts",
     "src/swagger-output.json",
     "src/**/*.ts", "src/**/*.json"
  ],
  "exclude": [
    "out-tsc",
    "dist",
    "jest.config.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.ts"
  ]
}
